datasource DS {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = "native"
}

model Task {
  id        Int        @id @default(autoincrement())
  task      String
  project   String
  start     DateTime?
  due       DateTime?
  createdAt DateTime   @default(now())
}
